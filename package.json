{
  "name": "inline-context",
  "version": "1.0.0",
  "main": "src/index.js",
  "scripts": {
    "start": "wp-scripts start",
    "build": "wp-scripts build",
    "lint:js": "wp-scripts lint-js",
    "lint:php": "phpcs --standard=WordPress *.php",
    "lint:php:fix": "phpcbf --standard=WordPress *.php",
    "lint": "npm run lint:js && npm run lint:php",
    "test": "npm run lint",
    "pot": "wp-scripts i18n --domain=inline-context --output languages/inline-context.pot 'src/**/*.js'",
    "prepackage": "npm run test && npm run build",
    "package": "bash scripts/package.sh",
    "prerelease": "npm run test && npm run build",
    "release": "bash scripts/package.sh"
  },
  "dependencies": {
    "react-quill": "^2.0.0",
    "dompurify": "^3.0.0"
  },
  "devDependencies": {
    "@wordpress/scripts": "^25.0.0"
  }
}
